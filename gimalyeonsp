import matplotlib.pyplot as plt
import streamlit as st

fig, ax = plt.subplots()

c1 = st.sidebar.radio('ì„ ì˜ ìƒ‰ì„ ì„ íƒí•˜ì‹œì˜¤',['red', 'green', 'blue', 'purple','orange'])
s1 = st.sidebar.radio('ì„ ì˜  ìŠ¤íƒ€ì¼ì„ ì„ íƒí•˜ì‹œì˜¤',['-','--',':','-.'])
m1 = st.sidebar.radio('ë§ˆì»¤ì˜ ìŠ¤íƒ€ì¼ì„ ì„ íƒí•˜ì‹œì˜¤',['o','^','s','p'])

love = []
y = []
for i in range(-20,21,3):
    love.append(i)
    y.append(-2*i*i + 3*i + 5)


plt.plot(love, y, color = c1, linestyle = s1, marker=m1)

st.pyplot(fig)
